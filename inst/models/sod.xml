   <species id="SODI"  initialConcentration="0.64"  compartment="cell" boundaryCondition="true"/>
   <species id="SOD"  initialConcentration="1e-05"  compartment="cell" boundaryCondition="true"/>
   <species id="cat"  initialConcentration="1e-05"  compartment="cell" boundaryCondition="true"/>
</listOfSpecies>
